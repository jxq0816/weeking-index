<mj-view>
	<mj-panel data-width="100%" data-height="100%">
		<mj-panel-head data-shrink="false" data-icon="fa fa-fire">
			导航菜单
		</mj-panel-head>
		<mj-panel-body>
			<mj-tree data-id="tree-tree" data-click="onTreeClick" data-border="true" data-autoload="true" data-action="js/mj/view/tree/data.json" data-icon-open="" data-icon-close="" data-icon-leaf="" data-method="get">
			</mj-tree>
		</mj-panel-body>
		<mj-panel-foot>
			<mj-button data-text="展开" data-click="expandFunc">
			</mj-button>
			<mj-button data-text="收起" data-click="collapseFunc">
			</mj-button>
		</mj-panel-foot>
	</mj-panel>
</mj-view>
