<mj-view data-scroll="iiii">
	<mj-panel data-height="100%">
		<mj-panel-head>
			查询条件
		</mj-panel-head>
		<mj-panel-body>	
			<mj-number data-column="2" data-click="onChange" data-label="mj-number" data-name="number" data-model="userName1" data-required="true" data-step="0.5" data-digit="1" data-placeholder="1212"></mj-number>
			<mj-file data-column="2" data-condition="condition" data-action="http://106.14.57.124/filemarket.server/services/file/upload" data-id="mj-file-0012" data-label="mj-file" data-name="userName1" data-required="true" data-min="0" data-max="100000" data-type="png/CSV" data-success="success" data-error="error" data-abort="abort" data-placeholder="1212wqw"></mj-file>
			<mj-textarea data-label="mj-textarea" data-rows="5" data-cols="10" data-name="mj-textarea" data-required="true" data-min="4" data-max="6" data-placeholder="1212wqw"></mj-textarea>
			<mj-url data-label="mj-url" data-click="onChange" data-name="url" data-required="true" data-placeholder="1212wqw"></mj-url>
			<mj-email data-label="mj-email" data-click="onChange" data-name="mj-email" data-required="true" data-placeholder="1212wqw"></mj-email>
			<mj-telephone data-label="mj-telephone" data-name="mj-telephone" data-required="true" data-placeholder="1212wqw"></mj-telephone>
			<mj-text data-label="mj-text" data-id="mj-text-123456" data-name="userName2" data-model="userName1" data-placeholder="1212wqw"></mj-text>
			<mj-password data-label="mj-password" data-name="userName12" data-required="true" data-min="20" data-max="30" data-placeholder="1212wqw"></mj-password>
			<mj-select  data-label="mj-select" data-id="mj-select11" data-name="mj-select"  data-empty="true" data-required="true"></mj-select>
			<mj-radio data-id="mj-radio111" data-change="aaa" data-value="2" data-disabled="false" data-label="性别" data-name="sex" data-action="{}" data-data='[{"id":"1","label":"一","value":"1"},{"id":"2","label":"二","value":"2"},{"id":"3","label":"三","value":"3"}]'></mj-radio>
			<mj-checkbox data-id="mj-checkbox111" data-change="test2"  data-column="1" data-value="1,2" data-disabled="false" data-label="水果" data-name="fruit" data-action="{}" data-data='[{"id":"1","label":"苹果","value":"1"},{"id":"2","label":"香蕉","value":"2"},{"id":"3","label":"桃子","value":"3"},{"id":"4","label":"梨子","value":"4"}]'></mj-checkbox>
			<mj-date data-id='sasadsadsa' data-type="date" data-column="1" data-change="aaa" data-required="true" data-format="yyyy-MM-dd" data-readonly="false" data-value="1489386120059" data-disabled="false" data-label="日期2222" data-name="date" data-placeholder="请选择日期"></mj-date>
			<mj-date data-column="1" data-change="aaa" data-required="true" data-format="yyyy-MM-dd" data-value="1489388319338" data-label="日期2222" data-name="date" data-placeholder="请选择日期"></mj-date>
			<mj-text data-label="mj-text"  data-id="mj-popup-123456" data-name="userName2" data-model="userName1" data-placeholder="1212wqw"></mj-text>
		</mj-panel-body>
		<mj-panel-foot>	
			<mj-button data-text="查询" data-click="clickFun">
			</mj-button>
			<mj-button data-text="重置" data-click="test1">
			</mj-button>
			<mj-button data-text="全选" data-click="test2">
			</mj-button>
			<mj-button data-text="取消" data-click="close">
			</mj-button>
			<mj-button data-text="设置只读" data-click="setReadOnly">
			</mj-button>
			<mj-button data-text="全部清除" data-click="clearItem">
			</mj-button>
			<mj-button data-text="加载数据" data-click="load">
			</mj-button>
			<mj-button data-text="初始化控件" data-click="initkj">
			</mj-button>
			<mj-button data-text="测试tooltip" data-id="tooltip-123" data-click="initpopup">
			</mj-button>
		</mj-panel-foot>
	</mj-panel>
</mj-view>
