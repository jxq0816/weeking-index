<mj-view>
	<mj-panel data-height="100%">
		<mj-panel-head data-collapsable="true" data-collapsed="false">
			表单
		</mj-panel-head>
		<mj-panel-body >
			<mj-form data-name="mainForm" data-editable="false" data-layout="horizontal" data-id="form-horizontal-form" data-column="2" data-label-width="150" data-label-align="right">
				<mj-slider data-border="false" data-value="2010" data-colspan="1" data-label="mj-slider-n" data-name="slider-n" data-required="true" data-min="20" data-max="30" data-from="2000" data-to="2017" data-step="1" data-format="n" data-finish="sliderChangeFunc">
				</mj-slider>
				<mj-slider data-value="30" data-colspan="1" data-label="mj-slider-p" data-name="slider-p" data-required="true" data-min="20" data-max="30" data-from="100" data-to="500" data-format="p">
					<mj-field-left data-width="50">fdas</mj-field-left>
					<mj-field-right data-width="50">fdas</mj-field-right>
				</mj-slider>
				<mj-text data-border="false" data-label-class="form-label" data-field-class="form-field" data-colspan="1" data-change="textChangeFunc" data-label="mj-text-1" data-name="form-text" data-readonly="false" data-required="true" data-min="20" data-max="30" data-placeholder="请输入...">
					<mj-field-left data-width="50" data-show-always="true">
						<mj-button data-height="100%" data-width="100%" data-text="a" data-icon="fa fa-book" data-click="clickFun"></mj-button>
					</mj-field-left>
					<mj-field-right data-width="50">
						<mj-button  data-height="100%" data-width="100%" data-text="a"></mj-button>
					</mj-field-right>
				</mj-text>
				<mj-url data-label="mj-url" data-click="onChange" data-name="url" data-required="true" data-placeholder="1212wqw" data-readonly="true">
					<mj-field-left data-width="50">
						<mj-button data-text="a"></mj-button>
					</mj-field-left>
					<mj-field-right data-width="50">
						<mj-button data-text="a"></mj-button>
					</mj-field-right>
				</mj-url>
				<mj-email data-label="mj-email" data-click="onChange" data-name="mj-email" data-required="true" data-placeholder="1212wqw"></mj-email>
				<mj-telephone data-label="mj-telephone" data-name="mj-telephone" data-required="true" data-placeholder="1212wqw"></mj-telephone>
				<mj-text data-label="mj-text" data-value="测试默认值" data-name="userName2" data-model="userName1" data-placeholder="1212wqw" data-disabled="false">
				<mj-field-left data-width="50">
						<mj-button data-text="a"></mj-button>
					</mj-field-left>
					<mj-field-right data-width="50">
						<mj-button data-text="a"></mj-button>
					</mj-field-right>
				</mj-text>
				<mj-password data-label="mj-password" data-name="userName12" data-required="true" data-min="20" data-max="30" data-placeholder="1212wqw"></mj-password>
				<mj-select data-model="single" data-value="1" data-label="select-single" data-empty="true" data-name="form-select-single" data-required="true" data-method="get"></mj-select>
				<mj-select data-model="multiple" data-label="select-multiple" data-empty="true" data-name="form-select-multiple" data-required="true" data-method="get"></mj-select>
				<mj-select-tree data-model="single" data-label="select-tree-single" data-empty="true" data-name="select-tree-single" data-required="true" data-method="get"  data-action="js/mj/view/tree/data.json"></mj-select-tree>
				<mj-select-tree data-model="multiple" data-label="select-tree-multiple" data-empty="true" data-name="select-tree-multiple" data-required="true" data-method="get"  data-action="js/mj/view/tree/data.json"></mj-select-tree>
				<mj-radio data-action="http://localhost/manjusaka/services/mradio/selection/list" data-change="aaa" data-value="1" data-disabled="false" data-label="性别" data-name="sex" data-method="get"></mj-radio>
				<mj-checkbox data-change="textChangeFunc" data-method="get" data-colspan="1" data-value="1,2"  data-label="水果" data-name="form-checkbox" ></mj-checkbox>
				<mj-date data-colspan="1" data-change="aaa" data-required="true" data-format="yyyy-MM-dd" data-value="1489388319338" data-readonly="false" data-disabled="false" data-label="日期" data-name="date" data-placeholder="请选择日期"></mj-date>
				<mj-number data-colspan="1" data-click="onChange" data-label="mj-number" data-name="number" data-model="userName1" data-required="true" data-min="3.8" data-max="6.2" data-step="0.2" data-digit="1" data-placeholder="1212"></mj-number>
				<mj-file data-colspan="2" data-label="mj-file" data-name="form-file" data-required="true" data-min="0" data-max="100000" data-type="png/CSV" data-success="success" data-error="error" data-abort="abort" data-placeholder="1212wqw"></mj-file>
				<mj-textarea data-label="mj-textarea" data-colspan="2" data-name="mj-textarea" data-required="true" data-min="4" data-max="60" data-placeholder="1212wqw" data-disabled="true"></mj-textarea>

			</mj-form>
		</mj-panel-body>
		<mj-panel-foot>
			<mj-button data-text="获取" data-icon="fa fa-book" data-click="clickFun">
			</mj-button>
			<mj-button data-text="重置" data-hide="true" data-click="test1">
			</mj-button>
			<mj-button data-text="设置view" data-click="setFormViewFunc">
			</mj-button>
			<mj-button data-text="设置edit" data-click="setFormEditFunc">
			</mj-button>
			<mj-button data-text="删除" data-click="deleteFunc">
			</mj-button>
			<mj-button data-text="只读" data-click="setReadOnly()">
			</mj-button>
		</mj-panel-foot>
	</mj-panel>
</mj-view>
