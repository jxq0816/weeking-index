<mj-view>
	<mj-panel data-id="panel-main" data-width="100%" data-height="100%" data-border="false">
		<mj-panel-head data-collapsable="false" data-style="background-color:red;">
			<span class="fa fa-bell">
				mj-panel-head
			</span>
		</mj-panel-head>
		<mj-panel-body>
			mj-panel-body
		</mj-panel-body>
		<mj-panel-foot>
			<mj-button data-text="展开" data-click="panelOpenFun"></mj-button>
			<mj-button data-text="收起" data-click="panelCloseFun"></mj-button>
		</mj-panel-foot>
	</mj-panel>
</mj-view>
