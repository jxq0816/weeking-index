<mj-view>
	<mj-table data-total-enable="false" data-fix="" data-home-showable="true" data-home-text="首页" data-prev-text="上一页" data-next-text="下一页" data-last-text="尾页" data-id="table-table" data-page="true" data-pageSize="true" data-action="js/mj/view/table/data.json" data-method="get" data-field="data" data-rowmousemove="rowmousemove" data-rowclick="rowclickFunc">
		<mj-table-toolbar>
			<mj-button data-text="window" data-click="winFunc" data-color="btn btn-primary"></mj-button>
			<mj-button data-text="alert" data-click="alertFunc" data-color="btn btn-primary"></mj-button>
			<mj-button data-text="confirm" data-click="confirmFunc" data-color="btn btn-primary"></mj-button>
			<mj-button data-text="loading" data-click="loadingFunc" data-color="btn btn-primary"></mj-button>
			<mj-button data-text="添加" data-click="insert" data-color="btn btn-primary"></mj-button>
		</mj-table-toolbar>

		<mj-table-columns data-model="multiple" data-index="false">
			<mj-table-column data-tooltip-showable="true" data-tooltip-render="changeCol2" data-title="col-1" data-field="name" data-align="center" data-width="" data-sort="true" data-render="changeCol1"></mj-table-column>
			<mj-table-column data-title="col-2" data-field="name" data-align="center" data-width="" data-sort="true" data-type="number" data-format="#.##"></mj-table-column>
			<mj-table-column data-tooltip-showable="true" data-tooltip-render="changeCol2"  data-title="col-3" data-field="name" data-align="center" data-width="" data-sort="" data-type="number" data-format="#,###"></mj-table-column>
			<mj-table-column data-title="col-4" data-field="name" data-align="center" data-width="" data-sort="" data-type="date" data-format="yyyy-MM-dd"></mj-table-column>
			<mj-table-column data-title="col-5" data-field="name" data-align="center" data-width="" data-sort="true"></mj-table-column>
			<mj-table-column data-title="col-6" data-field="name" data-align="center" data-width="" data-sort="true"></mj-table-column>
		</mj-table-columns>
	</mj-table>
</mj-view>
