<mj-view>
	<mj-layout data-width="100%" data-height="100%">
		<mj-layout-north data-switch="true" data-height="80" data-border="true">
			mj-layout-north
		</mj-layout-north>
		<mj-layout-east data-switch="true" data-scroll="false" data-width="200" data-border="false">
			mj-layout-east
		</mj-layout-east>
		<mj-layout-center data-scroll="false" data-border="false">
			mj-layout-center
		</mj-layout-center>
		<mj-layout-west data-switch="true" data-scroll="false" data-width="200" data-border="false">
			mj-layout-west
		</mj-layout-west>
		<mj-layout-south data-switch="true" data-scroll="false" data-height="80" data-split-showable="false">
			mj-layout-south
		</mj-layout-south>
	</mj-layout>
</mj-view>
